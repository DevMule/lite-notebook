%% md
# Hello!

Let's create a `div` right below:

%% fetch
text: ros_txt = notebooks/test/rosenblatt.py

%% code id=editor run

%% js
    var editor = ace.edit("editor");
    editor.getSession().setValue(ros_txt)

    let code_output = lite_notebook.elements.code_output();
    lite_notebook.screen.appendChild(code_output);

    code_output.attachEditor(editor);


